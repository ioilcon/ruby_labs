def elements_processing(list, func, accumulator = [], el_pr = Proc.new {true}, ind_pr = Proc.new {true}, ind = 0)
    if ind == list.length then
        accumulator
    else
        elements_processing(list, func, (el_pr(list[ind]) && ind_pr(ind))?func(list, ind, acc):accumulator, el_pr, ind, ind + 1)
    end
end

def one_element_processing(list, func, accumulator = [], pr = Proc.new {true})
    elements_processing(list, func, accumulator, pr).first()
end
